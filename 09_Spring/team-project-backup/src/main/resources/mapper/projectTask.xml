<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sh.haagendazo.bio.mapper.ProjectTaskMapper">
<resultMap type="ProjectTask" id="TaskParam">
		<id property="taskId" column="task_id" />
		<result property="projectId" column="project_id"/>
		<result property="taskName" column="task_name"/>
		<result property="assigneeId" column="assignee_id"/>
		<result property="dueDate" column="due_date"/>
		<result property="status" column="status"/>
		<result property="priority" column="priority"/>
		<result property="notes" column="notes"/>
</resultMap>

 <select id="allProjectTask" resultMap="TaskParam">
 	SELECT task_id, project_id, task_name, assignee_id, due_date, status, priority, notes FROM project_task
 </select>
 
</mapper>